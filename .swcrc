{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": true,
  "minify": false,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "development": true
      }
    },
    "externalHelpers": false,
    "keepClassNames": true,
    "baseUrl": ".",
    "paths": {}
  },
  "module": {
    "type": "es6"
  },
  "env": {
    "targets": {
      "chrome": "90",
      "firefox": "88",
      "safari": "14",
      "edge": "90"
    },
    "mode": "entry",
    "coreJs": "3.0"
  }
}
